    "bytes": "^3.1.2",
    "colorjs.io": "^0.5.2",
    "consola": "^3.4.2",
    "dayjs": "^1.11.19",
    "dice-roller-parser": "^0.1.8",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.3",
    "lg-fullscreen": "^1.2.1",
