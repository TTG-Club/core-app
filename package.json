{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "vue-tsc -p tsconfig.json",
    "lint:check": "eslint . --cache --cache-strategy content",
    "lint:write": "eslint . --cache --cache-strategy content --fix",
    "lint:workflow": "eslint . --cache --cache-strategy content --quiet",
    "stylelint:check": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .gitignore",
    "stylelint:write": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .gitignore --fix",
    "stylelint:workflow": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .gitignore --quiet",
    "pre-commit": "lint-staged",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@ant-design-vue/nuxt": "^1.4.2",
    "@nuxtjs/seo": "^2.0.0-rc.18",
    "@vueuse/core": "^11.0.1",
    "@vueuse/gesture": "^2.0.0",
    "@vueuse/math": "^11.0.1",
    "@vueuse/nuxt": "^11.0.1",
    "@vueuse/shared": "^11.0.1",
    "ant-design-vue": "^4.2.3",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "nuxt": "^3.12.4",
    "pinia": "^2.2.2",
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.1",
    "@nuxt/eslint-config": "^0.5.1",
    "@nuxtjs/device": "^3.1.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/robots": "^4.1.0",
    "@nuxtjs/sitemap": "^6.0.0-beta.1",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@pinia/nuxt": "^0.5.3",
    "@types/lint-staged": "^13.3.0",
    "@types/lodash-es": "^4.17.12",
    "dayjs-nuxt": "^2.1.9",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.9",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.77.8",
    "stylelint-config-clean-order": "^6.1.0",
    "typescript": "^5.5.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.0.29"
  }
}
