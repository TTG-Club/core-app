    "@prettier/plugin-oxc": "^0.1.3",
    "@tailwindcss/typography": "^0.5.19",
    "@types/bytes": "^3.1.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash-es": "^4.17.12",
