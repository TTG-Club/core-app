{
  "name": "ttg-core-app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm nuxt dev --host --no-qr",
    "build": "pnpm nuxt build",
    "preview": "pnpm nuxt preview",
    "server": "node .output/server/index.mjs",
    "postinstall": "pnpm nuxt cleanup && pnpm nuxt prepare",
    "type-check": "vue-tsc -p tsconfig.json",
    "lint:check": "eslint . --cache --cache-strategy content",
    "lint:write": "eslint . --cache --cache-strategy content --fix",
    "lint:workflow": "eslint . --cache --cache-strategy content --quiet",
    "stylelint:check": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .gitignore",
    "stylelint:write": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .gitignore --fix",
    "stylelint:workflow": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .gitignore --quiet",
    "pre-commit": "lint-staged",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@ant-design-vue/nuxt": "^1.4.6",
    "@aws-sdk/client-s3": "^3.816.0",
    "@faker-js/faker": "^9.8.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.9.0",
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/seo": "^3.0.3",
    "@pinia/nuxt": "^0.11.0",
    "@vueuse/core": "^13.2.0",
    "@vueuse/gesture": "^2.0.0",
    "@vueuse/math": "^13.2.0",
    "@vueuse/nuxt": "^13.2.0",
    "@vueuse/shared": "^13.2.0",
    "ant-design-vue": "^4.2.6",
    "bytes": "^3.1.2",
    "dayjs": "^1.11.13",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "mime": "^4.0.7",
    "motion": "^12.12.2",
    "ms": "^2.1.3",
    "normalize.css": "^8.0.1",
    "nuxt": "^3.17.4",
    "nuxt-security": "^2.2.0",
    "pinia": "^3.0.2",
    "sharp": "^0.34.2",
    "transliteration": "^2.3.5",
    "uuid": "^11.1.0",
    "vue": "^3.5.14",
    "zod": "^3.25.23"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@types/bytes": "^3.1.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lint-staged": "^13.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/ms": "^2.1.0",
    "@vue/language-server": "^2.2.10",
    "eslint": "^9.27.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.0.0",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.89.0",
    "stylelint-config-clean-order": "^7.0.0",
    "typescript": "^5.8.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=20.16",
    "pnpm": ">=10.7.0"
  },
  "packageManager": "pnpm@10.7.0"
}
